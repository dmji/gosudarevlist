package navigation

type class string

func (c class) String() string {
	return string(c)
}

const (
	navButtonSingleStyle   class = " rounded-full"
	navButtonGroupBegStyle class = " rounded-s-full"
	navButtonGroupEndStyle class = " rounded-e-full"
)

type navButtonType int

const (
	navButtonSingle navButtonType = iota
	navButtonGroupBeg
	navButtonGroupMiddle
	navButtonGroupEnd
)

func navButtonStyle(t navButtonType) class {
	switch t {
	case navButtonSingle:
		return navButtonSingleStyle
	case navButtonGroupBeg:
		return navButtonGroupBegStyle
	case navButtonGroupMiddle:
		return ""
	case navButtonGroupEnd:
		return navButtonGroupEndStyle
	}
	return ""
}

